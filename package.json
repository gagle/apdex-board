{
  "name": "apdex-board",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "clean": "concurrently \"rimraf dist\" \"rimraf coverage\" \"rimraf npm-debug.log*\" \"npm cache verify\" \"npm prune\"",
    "lint": "npm run lint:ts && npm run lint:scss",
    "lint:ts": "tsc --noEmit && eslint src --ext .ts --quiet --fix",
    "lint:scss": "stylelint ./src/**/*.scss --syntax scss --cache",
    "start": "webpack-dev-server --config webpack.config.js",
    "start:build": "ws --directory dist --hostname localhost --port 4000",
    "start:server": "node fixtures/server.js",
    "test": "jest"
  },
  "dependencies": {
    "@webcomponents/webcomponentsjs": "2.4.1",
    "whatwg-fetch": "3.0.0"
  },
  "devDependencies": {
    "@hapi/hapi": "18.4.0",
    "@types/jest": "24.0.25",
    "@types/node": "13.1.6",
    "@typescript-eslint/eslint-plugin": "2.15.0",
    "@typescript-eslint/parser": "2.15.0",
    "clean-webpack-plugin": "3.0.0",
    "concurrently": "5.0.2",
    "css-loader": "3.4.1",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-plugin-prettier": "3.1.2",
    "html-webpack-plugin": "3.2.0",
    "husky": "4.0.1",
    "jest": "24.9.0",
    "js-beautify": "1.10.2",
    "lint-staged": "9.5.0",
    "local-web-server": "3.0.7",
    "mini-css-extract-plugin": "0.9.0",
    "node-sass": "4.13.0",
    "postcss-loader": "3.0.0",
    "raw-loader": "4.0.0",
    "rimraf": "3.0.0",
    "sass-loader": "8.0.0",
    "style-loader": "1.1.2",
    "stylelint": "12.0.1",
    "stylelint-config-prettier": "7.0.0",
    "stylelint-config-standard": "19.0.0",
    "stylelint-order": "4.0.0",
    "stylelint-scss": "3.13.0",
    "ts-jest": "24.3.0",
    "ts-loader": "6.2.1",
    "tsconfig-paths-webpack-plugin": "3.2.0",
    "typescript": "3.7.4",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.10.1"
  }
}
